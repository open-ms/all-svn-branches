if test -n "$ac_init_help"; then
  case $ac_init_help in
     short | recursive ) echo "Configuration of OpenMS 1.2 :";;
   esac
  cat <<\_ACEOF

Optional Features:
  --disable-FEATURE       do not include FEATURE (same as --enable-FEATURE=no)
  --enable-FEATURE[=ARG]  include FEATURE [ARG=yes]
  --disable-optimization  disables optimizing the library for speed.
  --enable-debuginfo      add -g to the compiler flags (to create debug information). Disables optimization!
  --enable-stl-debug      enables debug mode of the STL when using gcc.
  --disable-pedantic      removes the '-pedantic' flag when using gcc.
