  --with-cgal-incl=DIR    CGAL header files are in DIR
  --with-cgal-libs=DIR    CGAL libraries are in DIR
  --with-mesa             use MESA instead of OpenGL
  --with-netcdf-incl=DIR  specify the location of the NetCDF header file "netcdf.h"
  --with-netcdf-libs=DIR  specify the location of the NetCDF archive files "libnetcdf.a" and "libnetcdf_c++.a"
  --with-andims-incl=DIR  specify the location of the ANDI/MS header file "ms10.h"
  --with-andims-libs=DIR  specify the location of the ANDI/MS archive file "ms10lib.a"
  --with-gsl-incl=DIR     specify the location of the directory containing "gsl/gsl_version.h"
  --with-gsl-libs=DIR     specify the location of the GSL library files "libgsl.so" and "libgslcblas.so"
  --with-libsvm-incl=DIR  specify the location of the libSVM directory containing "svm.h"
  --with-libsvm-libs=DIR  specify the location of the libSVM directory containing "svm.o"
  --with-xerces-incl=DIR  specify the location of the XERCES include directory containing "xercesc/util/Base64.hpp"
  --with-xerces-libs=DIR  specify the location of the XERCES lib directory containing "libxerces-c.so"
